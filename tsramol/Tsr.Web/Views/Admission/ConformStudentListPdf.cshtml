            <th>Name</th>
            <th>Course</th>
            <th>Batch</th>
            <th>Email</th>
            <th>Cell</th>
            <th>Fees Paid</th>
            
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ApplicationCode</td>
                <td>@item.Name</td>
                <td>@item.Course</td>
                <td>@item.Batch</td>
                <td>@item.Email</td>
                <td>@item.Cell</td>
                <td>@item.FeePaid</td>
            </tr>
        }
